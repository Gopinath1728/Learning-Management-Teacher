        android:label="@string/menu_view_attendance"
        tools:layout="@layout/fragment_student_attendance_data" />

    <fragment
        android:id="@+id/nav_add_results"
        android:name="com.example.sampleschoolteacher.ui.courses.class_data.examination.add_result.AddResultFragment"
        android:label="@string/menu_add_results"
        tools:layout="@layout/fragment_add_result" />

    <fragment
        android:id="@+id/nav_view_results"
        android:name="com.example.sampleschoolteacher.ui.courses.class_data.class_students.student_data.student_result.StudentResultFragment"
        android:label="@string/menu_view_results"
        tools:layout="@layout/fragment_student_result" />

</navigation>